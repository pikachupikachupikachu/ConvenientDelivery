<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--点击阅读全文与收起的自定义View-->
    <declare-styleable name="ClickShowMoreLayout">
        <attr name="show_line" format="integer" />
        <attr name="click_text" format="string" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>


    <!--显示点赞列表的控件-->
    <declare-styleable name="PraiseWidget">
        <attr name="click_bg_color" format="color" />
        <attr name="font_color" format="color" />
        <attr name="font_size" format="dimension" />
        <attr name="like_icon" format="reference" />
    </declare-styleable>

</resources>
